	top: 20px;
	left: 0px;
	float:left;
	cursor:pointer;
}

#btnCenter  {
	background-image: url(../img/summary/btnCenter.png);
	background-repeat: no-repeat;
	background-position: center center;
	width: 60px;
	height: 40px;
	top: 0px;
	left: 0px;
	float:left;
	text-align:center;

}


#btnNextsummary {
	background-image: url(../img/summary/btnNext.png);
	background-repeat: no-repeat;
	position: relative;
	width: 41px;
	height: 11px;
	top: 20px;
	left: 0px;
