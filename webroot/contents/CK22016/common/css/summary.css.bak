
/* 정리하기 */

.summaryBg {
		position: absolute;
		top: 0px;
		left: 0px;
		background-image: url(../img/summary/summaryBg.png);
		background-repeat: no-repeat;
		width:1000px;
		height:596px;
		overflow:hidden;


}


#sumSet{		
		position: absolute;
		top:30px;
		left: 60px;
		width: 970px;
		height: 500px;

}


#sumtitle{
		font-family:"Nanum Gothic";
		position: relative;
		top: 0px;
		left: 0px;
		height:100px;
		display:block;
}

